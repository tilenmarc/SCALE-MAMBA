def load_data():
    a1 = Array(1000, sint)
    a2 = Array(1000, sint)
    a3 = Array(1000, sint)
    a = Array(1000, sint)
    @for_range(1000)
    def f(i):
        a1[i] = sint.get_private_input_from(0, 0)
        a2[i] = sint.get_private_input_from(1, 1)
        a3[i] = sint.get_private_input_from(2, 2)
        a[i] = a1[i] + a2[i]
        a[i] = a[i] + a3[i]
        a[i] = a[i] % 2**32

    return a

def output_result(res):
    res.reveal_to(0)

def sum(a):
    res = Array(1000 + 1, sint)
    res[0] = sint(0)
    @for_range(1000)
    def f(i):
        res[i+1] = res[i] + a[i]
    return res[1000]

a = load_data()
s = sum(a)
output_result(s)

restart()
